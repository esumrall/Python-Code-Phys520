import numpy as np
import matplotlib.pyplot as plt

# METHOD 1
time = []
fluorescence = []

f = open('./WiCell_Data1.txt', 'r')

for row in f:
    row = row.split(' ')
    time.append(row[0])
    fluorescence.append(float(row[1]))

plt.plot(time, fluorescence)

f.close()

